#!/bin/bash

# Download the main script
echo "Downloading betang script..."
curl -# -o ~/betang.sh https://raw.githubusercontent.com/robertneed20k/betang/main/betang.sh

# Check if download was successful
if [ $? -eq 0 ]; then
    echo "betang script downloaded successfully!"
    echo "You can now run the script by typing 'betang' in your terminal."
else
    echo "Failed to download betang script. Please try again later."
fi
